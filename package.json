{
  "name": "snakeobj",
  "version": "0.0.7",
  "description": "Changes all properties to camel case, except for the provided exceptions.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "istanbul cover tape test/**/*.spec.js",
    "test-dev": "tape test/**/*.spec.js | tap-spec || echo ''",
    "tdd": "nodemon -q -x 'npm run test-dev'",
    "build": "babel index.js --presets babel-preset-es2015 --plugins transform-object-assign --out-dir dist",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=6.7.0"
  },
  "author": "Gustavo Ortiz @codingpains",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.1.0",
    "istanbul": "^0.4.4",
    "nodemon": "^1.9.2",
    "suitape": "^1.0.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "is-plain-object": "^2.0.1",
    "snake-case": "^2.1.0"
  }
}
